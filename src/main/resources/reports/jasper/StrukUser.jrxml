	<queryString language="SQL">
		<![CDATA[SELECT ui.id,
	ui.nama,
	ui.nomor_telpon,
	ui.alamat,
	ui.email,
	pe.id_pembeli,
	ps.id_pesanan,
	ps.waktu_selsai,
	ps.total_item,
	ps.total_harga,
	ps.status,
	r.id_restaurant,
	r.nama AS nama_restaurant, 
	r.alamat AS alamat_restaurant,
	me.subtotal_harga,
	me.jumlah_item,
	mr.nama AS nama_menu,
	mr.harga
FROM user_info AS ui
	JOIN pembeli AS pe ON 
	 ui.id = pe.id_pembeli 
	JOIN pesanan AS ps ON 
	 pe.id_pembeli = ps.id_pembeli 
	JOIN restaurant AS r ON 
	 ps.id_restaurant = r.id_restaurant 
	JOIN memesan AS me ON 
	 me.id_pesanan = ps.id_pesanan 
	JOIN menu_restaurant AS mr ON 
	 me.id_mr = mr.id_mr 
WHERE 
	 ps.status = 'Selsai' AND ps.id_pesanan =  $P{orderId}]]>
	</queryString>
